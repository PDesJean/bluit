<h1>Posts</h1>

<% @posts.each do |post| %>
<ul class="posts">
  <li>
    <div class="title">
      <a href="<%= post.link %>" target="_blank"><%= post.title %></a>
    </div>
    <div class="tagline" title=" <%= post.created_at %> ">
      submitted <%= time_ago_in_words post.created_at %> ago
    </div>
  </li>
<% end %>
</ul>
